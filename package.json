{
  "name": "palmares",
  "productName": "Palmarès",
  "version": "2.0.0",
  "releaseDate": "X/X/2017",
  "author": "Feugy <damien.feugas@gmail.com>",
  "description": "Palmarès [pal.ma.ʁɛs] is a desktop application to get french and internationnal ballroom dancing results",
  "repository": {
    "type": "git",
    "url": "https://github.com/feugy/palmares.git"
  },
  "dependencies": {
    "async": "1.5.0",
    "cheerio": "0.19.0",
    "csv": "0.4.6",
    "fs-extra": "0.26.2",
    "hogan.js": "3.0.2",
    "js-yaml": "3.4.3",
    "md5": "2.0.0",
    "moment": "2.10.6",
    "request": "2.65.0",
    "source-map-support": "0.4.15",
    "underscore": "1.8.3",
    "underscore.string": "3.2.2",
    "xlsx.js": "https://github.com/feugy/xlsx.js/archive/master.tar.gz"
  },
  "devDependencies": {
    "chai": "3.4.1",
    "coffee-script": "1.10.0",
    "electron": "1.6.7",
    "electron-builder": "18.1.0",
    "express": "4.13.3",
    "gulp": "3.9.0",
    "gulp-coffee": "2.3.1",
    "gulp-spawn-mocha": "2.2.1",
    "gulp-stylus": "2.1.0",
    "gulp-util": "3.0.7",
    "iconv-lite": "0.4.13",
    "istanbul": "0.4.0",
    "method-override": "2.3.5",
    "mocha": "2.3.3",
    "stylus": "0.52.4"
  },
  "scripts": {
    "dist": "build",
    "postinstall": "install-app-deps",
    "start": "electron .",
    "test": "gulp test"
  },
  "build": {
    "files": [
      "**/*",
      "!installer",
      "!src",
      "!test",
      "!installer",
      "!.gitignore",
      "!.gulpfile.js",
      "!log.txt",
      "!**/node_modules/*/{CHANGELOG.md,README.md,README,readme.md,readme,test,__tests__,tests,powered-test,example,examples,*.d.ts}",
      "!**/node_modules/.bin",
      "!**/*.{o,hprof,orig,pyc,pyo,rbc}",
      "!**/._*",
      "!**/{.DS_Store,.git,.hg,.svn,CVS,RCS,SCCS,__pycache__,thumbs.db,.gitignore,.gitattributes,.editorconfig,.flowconfig,.yarn-metadata.json,.idea,appveyor.yml,.travis.yml,circle.yml,npm-debug.log,.nyc_output,yarn.lock,.yarn-integrity}"
    ],
    "copyright": "Copyright 2013-2017 © Damien Simonin Feugas",
    "win": {
      "icon": "style/ribas-icon.ico",
      "publisherName": "Damien Simonin Feugas",
      "legalTrademarks": "Copyright 2013-2017 © Damien Simonin Feugas"
    },
    "nsis": {
      "perMachine": true,
      "language": "0012",
      "deleteAppDataOnUninstall": false,
      "script": "installer/custom.nsh"
    }
  },
  "main": "lib/main.js",
  "licence": "MIT",
  "engine": {
    "node": "4.0.0"
  }
}
